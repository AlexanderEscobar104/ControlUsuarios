<Window x:Class="ControlUsuarios.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlUsuarios"
        mc:Ignorable="d"
        Title="Mi Aplicacion" Height="345" Width="452">
    <Grid Margin="0,0,0,18">

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="43*"/>
            <ColumnDefinition Width="757*"/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="DGDatosPersonales"  d:ItemsSource="{d:SampleData ItemCount=5}" Margin="5,99,24,10" Grid.Column="1" CanUserResizeColumns="False"/>
        <Label Content="Lista De Usuarios" HorizontalAlignment="Center" Margin="0,23,0,0" VerticalAlignment="Top" FontSize="20" Grid.Column="1"/>

    </Grid>
</Window>
